Sub copyer()

With Application
    .ScreenUpdating = False
    .EnableEvents = False     'Отключение анимации
    .DisplayAlerts = False
End With

Worksheets("Check").Activate
RowLenCheck = ActiveSheet.UsedRange.Rows(ActiveSheet.UsedRange.Rows.Count).Row  'count of rows
Dim i As Integer 'Cycle counter
Dim z As Integer 'Counter to insert in "not" algorithms
z = 1
Dim x As Integer 'Counter to insert in "not" hash
x = 1

For i = 1 To RowLenCheck Step 1
    Cells(i, 4).Select
    If ActiveCell > 0 Then
    
        ActiveSheet.Range(Cells(i, 4), Cells(i, 6)).Select
        Selection.copy
            
        Worksheets("not").Activate
        ActiveSheet.Range(Cells(z, 1), Cells(z, 2)).Select
        ActiveCell.PasteSpecial
        
        Worksheets("Check").Activate   'Копируем категорию
        Cells(i, 7).Select
        ActiveCell.copy
        
        Worksheets("not").Activate
        Cells(z, 2).Select
        ActiveCell.PasteSpecial
        
        z = z + 1
        
        Worksheets("Check").Activate
        
        End If
        
Next i
For j = 1 To RowLenCheck Step 1 ' Алгоритм для хэшей
    Cells(j, 5).Select
    
    If ActiveCell > 0 Then
        ActiveSheet.Range(Cells(j, 5), Cells(j, 6)).Select 'Копируем название хэша и длину и вставляем в нот
        Selection.copy
            
        Worksheets("not").Activate
        ActiveSheet.Range(Cells(x, 4), Cells(x, 6)).Select
        ActiveCell.PasteSpecial
            
        Worksheets("Check").Activate
        Cells(j, 7).Select
        ActiveCell.copy
            
        Worksheets("not").Activate
        Cells(x, 6).Select
        ActiveCell.PasteSpecial
        
        x = x + 1
        
        Worksheets("Check").Activate
        
    End If
Next j


Worksheets("not").Activate
Dim rg As Range
Set rg = Range("A1").CurrentRegion                                                  'Форматирование алгоритмов
rg.RemoveDuplicates Columns:=Array(1, 2, 3), Header:=xlYes

Dim rg2 As Range
Set rg2 = Range("D1").CurrentRegion
rg2.RemoveDuplicates Columns:=Array(4, 5, 6), Header:=xlYes          'Форматирование хэшей

Worksheets("not").Activate
RowLenNot = ActiveSheet.UsedRange.Rows(ActiveSheet.UsedRange.Rows.Count).Row
Dim o As Integer
o = 1
For o = 1 To RowLenNot Step 1

    Worksheets("not").Activate
    Cells(o, 1).Select
    Selection.copy
    
    Worksheets("notification").Activate
    ActiveSheet.Cells(o, 1).Select
    ActiveCell.PasteSpecial
    
    Worksheets("not").Activate
    Cells(o, 2).Select
    Selection.copy
    
    Worksheets("notification").Activate
    ActiveSheet.Cells(o, 5).Select
    ActiveCell.PasteSpecial
    
    Worksheets("not").Activate
    Cells(o, 3).Select
    Selection.copy
    
     Worksheets("notification").Activate
    ActiveSheet.Cells(o, 3).Select
    ActiveCell.PasteSpecial
    
Next o
End Sub
